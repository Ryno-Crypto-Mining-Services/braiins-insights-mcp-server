{
  "name": "@ryno-crypto/braiins-insights-mcp-server",
  "version": "0.2.0",
  "description": "Model Context Protocol server for Braiins Insights Dashboard API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "braiins-insights-mcp": "./dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --testMatch='**/tests/unit/**/*.test.ts'",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testMatch='**/tests/unit/**/*.test.ts'",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --testMatch='**/tests/integration/**/*.test.ts'",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --testMatch='**/tests/e2e/**/*.test.ts'",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --testMatch='**/tests/unit/**/*.test.ts'",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "clean": "rm -rf dist coverage",
    "prebuild": "npm run clean",
    "prepare": "husky && npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "braiins",
    "bitcoin",
    "mining",
    "insights",
    "api",
    "claude",
    "cursor",
    "copilot"
  ],
  "author": "Ryno Crypto Mining Services",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ryno-Crypto-Mining-Services/braiins-insights-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/Ryno-Crypto-Mining-Services/braiins-insights-mcp-server/issues"
  },
  "homepage": "https://github.com/Ryno-Crypto-Mining-Services/braiins-insights-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "winston": "^3.11.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
